<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane onMouseClicked="#clickOnPane" opacity="0.7" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.kodilla.first.individual.project.controllers.CleanController">
    <children>
        <Button fx:id="playBtn" defaultButton="true" layoutX="401.0" layoutY="245.0" mnemonicParsing="false"
                onAction="#play" prefHeight="36.0" prefWidth="171.0" text="Graj!">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="401.0" layoutY="333.0" mnemonicParsing="false" onAction="#backMenu" prefHeight="44.0" prefWidth="171.0" text="Menu">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="401.0" layoutY="289.0" mnemonicParsing="false" onAction="#settingsMenu" prefHeight="44.0" prefWidth="171.0" text="Ustawienia">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
